<div class="card" id="employees">
    
    <div class="card-title-container">
    <h2 class='card-title'>{{name}}</h2>
    <h5 class='card-text'><i class="fas fa-glasses"></i> {{title}}</h5>
    </div>
    <div class="card">
        <ul class="list-group list-group-flush">
            <li class="list-group-item" id="id">ID: {{id}}</li>
            <li class="list-group-item" id="email">Email: {{email}}</li>
            <li class="list-group-item" id="github">GitHub: <a href="https://github.com/{{github}}">{{github}}</a></li>
        </ul>
    </div>
    
</div>